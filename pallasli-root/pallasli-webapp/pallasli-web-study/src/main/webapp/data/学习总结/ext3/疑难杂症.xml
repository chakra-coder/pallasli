  <knowledge> 
    <id>1</id>  
    <caption>Google等浏览器grid对齐问题</caption>  
    <content>&lt;div&gt;Ext.override(Ext.grid.GridView,{&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; getColumnStyle : function(colIndex, isHeader) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var colModel &amp;nbsp;= this.cm,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; colConfig = colModel.config,&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; style &amp;nbsp; &amp;nbsp; = isHeader ? '' : colConfig[colIndex].css || '',&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; align &amp;nbsp; &amp;nbsp; = colConfig[colIndex].align;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if(Ext.isChrome){&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; style += String.format("width: {0};", parseInt(this.getColumnWidth(colIndex))-2+'px');&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }else{&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; style += String.format("width: {0};", this.getColumnWidth(colIndex));&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (colModel.isHidden(colIndex)) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; style += 'display: none; ';&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (align) {&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; style += String.format("text-align: {0};", align);&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return style;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; }&lt;/div&gt;&lt;div&gt;});&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;</content> 
  </knowledge>  
  <knowledge> 
    <id>2</id>  
    <caption>form嵌套form问题</caption>  
    <content>&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;Ext.override(Ext.Panel,{&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp;createElement : function(name, pnode){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if(this[name]){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; pnode.appendChild(this[name].dom);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if(name === 'bwrap' || this.elements.indexOf(name) != -1){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if(this[name+'Cfg']){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;//if(up("form"&amp;amp;&amp;amp;isform)){&lt;/font&gt;&lt;span style="white-space: pre; "&gt; &lt;/span&gt;this[name+'Cfg']["tag"]="div";}&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; try{&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space:pre"&gt; &lt;/span&gt;this[name] = Ext.fly(pnode).createChild(this[name+'Cfg']);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space:pre"&gt; &lt;/span&gt;}catch(e){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space:pre"&gt; &lt;/span&gt;this[name+'Cfg']["tag"]="div";&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space:pre"&gt; &lt;/span&gt;this[name] = Ext.fly(pnode).createChild(this[name+'Cfg']);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space:pre"&gt; &lt;/span&gt;}&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }else{&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; var el = document.createElement('div');&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; el.className = this[name+'Cls'];&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; this[name] = Ext.get(pnode.appendChild(el));&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if(this[name+'CssClass']){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; this[name].addClass(this[name+'CssClass']);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if(this[name+'Style']){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; this[name].applyStyles(this[name+'Style']);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; }&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;});&lt;/font&gt;&lt;/div&gt;</content> 
  </knowledge>  
  <knowledge> 
    <id>id_knowledge_0000000003</id>  
    <caption>禁止IE的backspace键(退格键)，但输入文本框时不禁止</caption>  
    <content>&lt;span style="font-family: Tahoma; font-size: 14px; line-height: 24px; "&gt;禁止IE的backspace键(退格键)，但输入文本框时不禁止&lt;/span&gt;&lt;br style="margin: 0px; padding: 0px; font-family: Tahoma; font-size: 14px; line-height: 24px; "&gt;&lt;p style="margin: 0px; padding: 0px; font-family: Tahoma; font-size: 14px; line-height: 24px; "&gt;&lt;a name="baidusnap0" style="margin: 0px; padding: 0px; color: rgb(23, 62, 128); "&gt;&lt;/a&gt;&lt;b style="margin: 0px; padding: 0px; background-color: rgb(255, 255, 102); "&gt;Ext&lt;/b&gt;实现方式：&lt;/p&gt;&lt;pre class="js" name="code" style="margin-top: 0px; margin-bottom: 0px; padding: 0px; font-size: 14px; line-height: 24px; "&gt;&lt;b style="margin: 0px; padding: 0px; background-color: rgb(255, 255, 102); "&gt;Ext&lt;/b&gt;.getDoc().on('keydown',function(e){ if(e.getKey() == 8 &amp;amp;&amp;amp; e.getTarget().type =='text' &amp;amp;&amp;amp; !e.getTarget().readOnly){ }else if(e.getKey() == 8 &amp;amp;&amp;amp; e.getTarget().type =='textarea' &amp;amp;&amp;amp; !e.getTarget().readOnly){ }else if(e.getKey() == 8){ e.preventDefault(); } });&lt;/pre&gt;&lt;p style="margin: 0px; padding: 0px; font-family: Tahoma; font-size: 14px; line-height: 24px; "&gt;&amp;nbsp;&lt;/p&gt;&lt;p style="margin: 0px; padding: 0px; font-family: Tahoma; font-size: 14px; line-height: 24px; "&gt;JavaScript实现方式：&lt;/p&gt;&lt;pre class="js" name="code" style="margin-top: 0px; margin-bottom: 0px; padding: 0px; font-size: 14px; line-height: 24px; "&gt;function document.onkeydown() { if ((event.keyCode==8) ) //屏蔽退格删除键 { if (window.event.srcElement.tagName.toUpperCase()!="INPUT" &amp;amp;&amp;amp; window.event.srcElement.tagName.toUpperCase()!="TEXTAREA" &amp;amp;&amp;amp; window.event.srcElement.tagName.toUpperCase()!="TEXT") { event.keyCode=0; event.returnValue=false; } } }&lt;/pre&gt;</content> 
  </knowledge>  
  <knowledge> 
    <id>4</id>  
    <caption>grid 序号列宽度自适应</caption>  
    <content>&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;div&gt;Ext.override(Ext.grid.GridPanel,{&lt;/div&gt;&lt;div&gt;onRender : function(ct, position){&lt;/div&gt;&lt;div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&amp;nbsp; &amp;nbsp; Ext.grid.GridPanel.superclass.onRender.apply(this, arguments);&lt;/div&gt;&lt;/div&gt;&lt;div&gt;后添加&lt;/div&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;br&gt;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;var self=this;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; this.getStore().on('load', function(t,records,o){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space:pre"&gt; &lt;/span&gt;var so = t.lastOptions;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;var sop = so?so.params : null;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;var sonumber = ((sop &amp;amp;&amp;amp; sop.start &amp;amp;&amp;amp; sop.limit)?sop.start : 0)+ records.length;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;if(sonumber &amp;gt; 1 &amp;amp;&amp;amp; sonumber &amp;lt; 51){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style="white-space:pre"&gt; &lt;/span&gt; self.getColumnModel().setColumnWidth(0, 28);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;}&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;else if(sonumber &amp;gt; 51 &amp;amp;&amp;amp; sonumber &amp;lt; 100){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style="white-space:pre"&gt; &lt;/span&gt; self.getColumnModel().setColumnWidth(0, 35);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;}&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;else if(sonumber &amp;gt; 9999){&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style="white-space:pre"&gt; &lt;/span&gt; self.getColumnModel().setColumnWidth(0, 42);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;}&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;else{&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &lt;span style="white-space:pre"&gt; &lt;/span&gt; self.getColumnModel().setColumnWidth(0, 30);&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;}&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font face="tahoma, arial, verdana, sans-serif"&gt;&lt;span style="white-space: pre; "&gt; &lt;/span&gt;});&lt;/font&gt;&lt;/div&gt;</content> 
  </knowledge> 
