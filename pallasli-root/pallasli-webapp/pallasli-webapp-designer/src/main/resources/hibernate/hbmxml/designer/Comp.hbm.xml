<class 
    name="com.pallas.designer.bean.Comp" 
    table="t_comp"
    lazy="false"
>

    <id
        name="id"
        type="java.lang.Long"
        column="id"
    >
    
        <generator class="native" />
    </id>

    <property
        name="compName"
        type="java.lang.String"
        column="comp_name"
        not-null="true"
        length="30"
    />
    <property
        name="compCaption"
        type="java.lang.String"
        column="comp_caption"
        not-null="true"
        length="30"
    />
    <property
        name="compTypeId"
        type="long"
        column="comp_type_id"
        not-null="true"
        length="19"
    />
    <property
        name="compClass"
        type="java.lang.String"
        column="comp_class"
        not-null="true"
        length="30"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
