    <property
        name="pageTypeCode"
        type="java.lang.String"
        column="page_type_code"
        not-null="true"
        unique="true"
        length="2"
    />
    <property
        name="pageTypeCaption"
        type="java.lang.String"
        column="page_type_caption"
        not-null="true"
        length="60"
    />
    <property
        name="version"
        type="int"
        column="version"
        not-null="true"
        length="10"
    />

